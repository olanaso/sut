                @*@Html.HiddenFor(model => model.Numero_Resolucion_Revocatoria)*@



                <div class="callout callout-warning" style="height: inherit;padding: inherit;display: block;">
                    Toda la información registrada se encuentra sujeta a verificación posterior
                </div>
                <div id="mensajesTitulo"></div>

                <div class="form-group">
                    <label class="control-label col-sm-6 col-xs-12">Nivel de riesgo(asignada según matriz de riesgo) : </label>
                    <div class="col-sm-6 col-xs-12">
                        @Html.DropDownListFor(model => model.NivelRiesgo, ViewBag.Nivel as List<SelectListItem>, new { @class = "form-control input-sm" })
                    </div>
                </div>

            }

