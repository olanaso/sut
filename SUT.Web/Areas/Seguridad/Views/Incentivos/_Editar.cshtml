                        <div class="col-sm-3 col-xs-2">

                            @if (Model.IncentivosId == 0)
                            {
                                <input type="text" name="Fec_Ing_Sol" class="form-control input-sm" value="" />
                            }
                            else
                            {
                                <input type="text" name="Fec_Ing_Sol" class="form-control input-sm" value="@MvcHtmlString.Create(Model.Fec_Ing_Sol == null ? "" : string.Format("{0:D2}/{1:D2}/{2}", Model.Fec_Ing_Sol.Value.Day, Model.Fec_Ing_Sol.Value.Month, Model.Fec_Ing_Sol.Value.Year))" />
                            }
                        </div>
                        <div class="col-sm-5 col-xs-12" style="padding-left: 0px">
                            @using (Html.BeginForm("", "", FormMethod.Post, new { @id = "form-archivo-adjunto1", @enctype = "multipart/form-data" }))
                            {
                                @Html.HiddenFor(model => model.Archivo_Fec_Ing_Sol)
                                <div class="col-sm-18 col-xs-12 file-list @MvcHtmlString.Create(Model.ArchivoAdjunto != null ? "" : "file-empty")">
                                    <ul>
                                        <li>
                                            <span class="text">
                                                <i class="fa fa-file"></i>
                                                <a href="@MvcHtmlString.Create(Model.ArchivoAdjunto != null ? @Url.Content(string.Format("~/General/Adjunto/Descargar/{0}", Model.Archivo_Fec_Ing_Sol)) : "")" target="_blank">
                                                    @MvcHtmlString.Create(Model.ArchivoAdjunto != null ? Model.ArchivoAdjunto.NombreArchivo : "")
                                                </a>
                                            </span>
                                            <div class="tools">
                                                <i class="fa fa-remove"></i>
                                            </div>
                                        </li>
                                    </ul>
                                    @Html.TextBox("PostedFile", "", new { @type = "file", @class = "form-control" })
                                </div>
                            }
                        </div>
                    </div>

