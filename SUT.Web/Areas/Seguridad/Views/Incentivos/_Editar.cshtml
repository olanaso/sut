@model Sut.Entities.Incentivos
<div class="modal-dialog ">
    <div class="modal-content" style="width: 130%;">
        <div class="modal-header">
            @*<button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>*@
            <h4 class="modal-title">@ViewBag.Title</h4>
        </div>
        <div class="modal-body form-horizontal">
            @using (Html.BeginForm("Guardar", "Seguridad/Incentivos", FormMethod.Post, new { @id = "form-modal-editar" }))
            {
                @Html.HiddenFor(model => model.IncentivosId)
                @Html.HiddenFor(model => model.NivelRiesgo)

                @Html.HiddenFor(model => model.NumRUC)
                @Html.HiddenFor(model => model.Actividadogironegocio)
                @Html.HiddenFor(model => model.Girocomplementario)
                @Html.HiddenFor(model => model.Funcion)
                @Html.HiddenFor(model => model.Numeral)
                @*@Html.HiddenFor(model => model.NivelInicial)*@
                @*@Html.HiddenFor(model => model.NivelFinal)*@
                @Html.HiddenFor(model => model.NumExpediente)
                @Html.HiddenFor(model => model.Fec_Ing_Sol)
                @Html.HiddenFor(model => model.Archivo_Fec_Ing_Sol)
                @Html.HiddenFor(model => model.Fec_Emision_Cert_ITSE)
                @Html.HiddenFor(model => model.Fec_Notificacion_Cert_ITSE)
                @Html.HiddenFor(model => model.Archivo_Fec_Emision_Cert_ITSE)
                @Html.HiddenFor(model => model.Archivo_Fec_Emision_Licencia_Funcionamiento)
                @Html.HiddenFor(model => model.Archivo_Fec_Notificacion_Cert_ITSE)
                @Html.HiddenFor(model => model.Archivo_Fec_Revocacion_Licencia_Funcionamiento)


                @Html.HiddenFor(model => model.Archivo_Fec_Notificacion_Licencia_Funcionamiento)

                @Html.HiddenFor(model => model.Estado)
                @Html.HiddenFor(model => model.Fec_Emision_Licencia_Funcionamiento)
                @Html.HiddenFor(model => model.Fec_Notificacion_Licencia_Funcionamiento)
                @Html.HiddenFor(model => model.Fec_Revocacion_Licencia_Funcionamiento)
                @Html.HiddenFor(model => model.Cod_Acreditacion_Inspector_ITSE)
                @Html.HiddenFor(model => model.Renovacion_ITSE)
                @*@Html.HiddenFor(model => model.Numero_Revocacion_Licencia_Funcionamiento)*@

                @Html.HiddenFor(model => model.Archivo_Fec_Suspencion_ITSE_Subsanacion)
                @Html.HiddenFor(model => model.Archivo_Fec_Suspencion_ITSE)
                @Html.HiddenFor(model => model.Archivo_Fec_Resolucion_denegatoria)
