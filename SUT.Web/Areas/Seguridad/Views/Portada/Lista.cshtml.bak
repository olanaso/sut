                listarcomunicado: function (page) {

                    table = $('#tblListaComunicado');
                    var model = {
                        pageIndex: page,
                        pageSize: table.data('pagesize')
                    };
                    $.ajax({
                        //url: "/Seguridad/Portada/GetAllLikePagincomunicado",
                        url: '@Url.Content("~/Seguridad/Portada/GetAllLikePagincomunicado")',
                        type: "GET",
                        data: model,
                        dataType: "json",
                        beforeSend: function () {
                            sut.wait.appendOverlay('#boxLista');
                        },
                        success: function (data) {
                            sut.wait.removeOverlay('#boxLista');
                            table.data('page', page);
                            table.data('totalrows', data.totalRows);
                            sut.portada.generarcomunicado(data.lista);
                            if (data.totalRows == 0) $('#tblListaComunicado tbody').append("<tr><td colspan='5' class='text-center text-bold'>NO SE ENCONTRARON REGISTROS.</td></td>");
                        },
                        error: function (data) {
                            sut.error.show('mensajes', data.responseText);
                        }
                    });
                },

                generarcomunicado: function (lista) {
                    var table = $('#tblListaComunicado');
                    var tabla = table.children('tbody');
                    tabla.children('tr').remove();
                    var rows = '';
                    var numero = (table.data('pagesize') * table.data('page')) - table.data('pagesize');
                    $.each(lista, function (i, r) {
                        rows += sut.string.format('<tr><td class="colNro" >{0}</td>', ++numero);
                        rows += sut.string.format('<td>{0}</td>', r.descripcion);
                        rows += sut.string.format('<td style="text-align: center;width:100px">{0}</td>', r.fecCreacion);
                        rows += sut.string.format('<td style="width:5px" class="text-center"><small class="label {0}">{1}</small></td>', sut.portada.estadoExpColor[r.estado], sut.portada.estado[r.estado]);
                        rows += '<td>';
                        rows += '<div class="btn-group">';
                        rows += sut.string.format('<a class="btn btn-success btn-xs" data-toggle="tooltip" data-placement="left" title="Editar" href="javascript:sut.portada.editar({0})" ><i class="fa fa-pencil"></i></a>', r.comunicadoID);
                        rows += '</div>';
                        rows += '</td>';
