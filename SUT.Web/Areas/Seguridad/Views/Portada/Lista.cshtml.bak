                listarentidadevaluacionBandeja: function (page) {

                    table = $('#tblListaentidadevaluacion');
                    var model = {
                        pageIndex: page,
                        pageSize: table.data('pagesize')
                    };
                    $.ajax({
                        //url: "/Seguridad/Portada/GetAllLikePaginnorma",
                        url: '@Url.Content("~/Seguridad/Portada/GetAllLikePaginentidadevaluacion")',
                        type: "GET",
                        data: model,
                        dataType: "json",
                        beforeSend: function () {
                            sut.wait.appendOverlay('#boxLista');
                        },
                        success: function (data) {
                            sut.wait.removeOverlay('#boxLista');
                            table.data('page', page);
                            table.data('totalrows', data.totalRows);
                            sut.portada.generarentidadevaluacionBandeja(data.lista);
                            //if (data.totalRows == 0) $('#tblListaentidadevaluacion tbody').append("<tr><td colspan='4' class='text-center text-bold'>NO SE ENCONTRARON REGISTROS.</td></td>");
                        },
                        error: function (data) {
                            sut.error.show('mensajes', data.responseText);
                        }
                    });
                },


                generarentidadevaluacionBandeja: function (lista) {
                    var table = $('#tblListaentidadevaluacion');
                    var tabla = table.children('tbody');
                    tabla.children('tr').remove();
                    var rows = '';
                    var numero = (table.data('pagesize') * table.data('page')) - table.data('pagesize');

                    $.each(lista, function (i, r) {
                        rows += sut.string.format('<tr><td class="colNro" >{0}</td>', ++numero);
                        rows += sut.string.format('<td>{0}</td>', r.NomEntidad);
                        rows += sut.string.format('<td>{0}</td>', r.Codigo);
                        //rows += sut.string.format('<td>{0}</td>', r.EstadoExpediente);
                        rows += sut.string.format('<td class = "text-center"><small class="label {0}">{1}</small></td>', sut.portada.estadoExpColor[r.EstadoExpediente], sut.portada.estadoExpediente[r.EstadoExpediente]);
                        rows += sut.string.format('<td style="text-align: center;">{0}</td>', r.FecEnvio);
                        rows += sut.string.format('<td style="text-align: center;">{0}</td>', r.DiasTranscurrido);
                        rows += sut.string.format('<td style="text-align: center;">{0}</td>', r.FecUltimaRevision);
                        rows += '<td style="text-align: center;">';
                        rows += '<div class="btn-group">';
                        if(r.Tomar==0)
                        {
                            rows += sut.string.format('<a class="btn btn-primary btn-xs"  data-placement="left" title="Revisar" href="javascript:sut.portada.guardaraccion2({0},1,1,{1})" ><i class="fa fa-registered"></i> </a>', r.NotificacionExpedientesId,r.ExpedienteId);
                        }
                        else  if(r.Tomar==1)
                        {
                            rows += sut.string.format('<a class="btn btn-danger btn-xs"  data-placement="left" title="Detener" href="javascript:sut.portada.guardaraccion2({0},0,3,{1})" ><i class="fa fa-stop"></i> </a>', r.NotificacionExpedientesId,r.ExpedienteId);
                        }
                        rows += '</div>';
                        rows += '</td>';

                        rows += sut.string.format('<td class = "text-center"><small class="label {0}">{1}</small></td>', sut.portada.estadoRevColor[r.RevEstado], sut.portada.estadoRevision[r.RevEstado]);
                        rows += '<td style="text-align: center;">';
                        rows += '<div class="btn-group">';
                        if(r.Tomar==1 || r.Tomar==2 )
                        {
                            ////rows += sut.string.format('<a class="btn btn-success btn-xs"  data-placement="left" title="Editar" href="@Url.Content("~/Simplificacion/Expediente/EditarObservacion/{0}")" ><i class="fa fa-pencil"></i></a>', r.ExpedienteId);
                            rows += sut.string.format('<a class="btn btn-success btn-xs"  data-placement="left" title="Editar" href="javascript:sut.portada.ActualizarRevision({0},{1})" ><i class="fa fa-pencil"></i></a>', r.NotificacionExpedientesId, r.ExpedienteId);
                        }
                        rows += '</div>';
                        rows += '</td>';
                        rows += '<td style="text-align: center;">';
                        rows += '<div class="btn-group">';
                        rows += sut.string.format('<a class="btn btn-dropbox btn-xs" data-toggle="tooltip" data-placement="left" title="Editar" href="javascript:sut.portada.editarAsignar({0})" ><i class="fa fa-users"></i></a>', r.ExpedienteId);
                        rows += '</div>';
                        rows += '</td>';
                        rows += '</tr>';
                    });
                    sut.pagination.update(table);
                    tabla.append(rows);
                },
