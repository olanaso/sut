@section Script {
    <script>

        var div = document.getElementById('capa');
        var but = document.getElementById('boton');

        //la funcion que oculta y muestra
        function showHide(e) {
            e.preventDefault();
            e.stopPropagation();
            if (div.style.display == "none") {
                div.style.display = "block"; sut.Roles.listarcategoria('tblBuscarUndOrganicacat', 0);
            } else if (div.style.display == "block") {
                div.style.display = "none";
            }
        }



        sut.Roles = {
            listarcategoria: function (table, page) {
                debugger;
                table = $('#' + table);

                $.ajax({
                    url: '@Url.Content("~/Seguridad/Roles/GetAllListaMenuRol")',
                    type: 'GET',
                    data: { RolId:page },
                    dataType: 'json',
                    beforeSend: function () {
                        sut.wait.appendProgress('#buscarUnidadOrganicacat .modal-footer');
                    },
                    success: function (data) {
                        sut.wait.removeProgress('#buscarUnidadOrganicacat .modal-footer');
                        table.data('page', page);
                        table.data('totalrows', data.totalRows);

                        debugger;
                        sut.Roles.generarmenu(table, data.lista);
                        //if (data.totalRows == 0)
                        //$('#tblBuscarUndOrganicacat tbody').append('<tr><td colspan="2" class="text-center text-bold">NO EXISTEN REGISTROS</td></td>');
                    },
                    error: function (data) {
                        sut.error.show('mensajes', data.responseText);
                    }
                });




            }, generarmenu: function (table, lista) {
                debugger;
                var tabla = table.children('tbody');
                tabla.children('tr').remove();
                var rows = '';
                var numero = (table.data('pagesize') * table.data('page')) - table.data('pagesize');
                $.each(lista, function (i, r) {

                    rows += sut.string.format('<tr class="rowselect" ><td><input id="chkbuscarUndOrg{1}" class="icheck" type="checkbox" value="{1}|{0}" /></td><td>{0}</td></tr>',
                        r.Descripcion, r.MenuId);

                });
                sut.pagination.update(table);
                tabla.append(rows);


            },
            editar: (id) => {

                $.ajax({
                    type: "GET",
                    //url: '/Seguridad/Miembro/Editar',
                    url: '@Url.Content("~/Seguridad/Roles/Editar")',
                    data: { id: id },
                    cache: false,
                    beforeSend: () => {
                        sut.wait.modal('ventana-container');
                        $('#modal-ventana').modal('show');
                    },
                    success: (data) => {
                        $('#ventana-container').html(data);
                    },
                    error: (result) => {
                        $('#modal-ventana').modal('hide');
                        sut.error.show('mensajes', result.responseText);
                    },
                    statusCode: {
                        203: () => { window.location.reload(); }
                    }
                });
            }, 
            listar: (page) => {
                table = $('#tblLista');
                var model = {
                    pageIndex: page,
                    pageSize: table.data('pagesize')
                };
                $.ajax({
                    //url: "/Seguridad/Miembro/GetAllLikePagin",
                    url: '@Url.Content("~/Seguridad/Roles/GetAllLikePagin")',
                    type: "GET",
                    data: model,
                    dataType: "json",
                    beforeSend: () => {
                        sut.wait.appendOverlay('#boxLista');
                    },
                    success: (data) => {
                        sut.wait.removeOverlay('#boxLista');
                        table.data('page', page);
                        table.data('totalrows', data.totalRows);
                        sut.Roles.generar(data.lista);
                        debugger;
                        if (data.totalRows == 0)  $('#tblLista tbody').append("<tr><td colspan='14' class='text-center text-bold'>NO SE ENCONTRARON REGISTROS.</td></td>");

                        debugger;

                        if (data.totalRows < 2)
                        {
                            $('#mensaje2').hide();
                            $('#mensaje1').show();
                        }else
                        {
                            $('#mensaje2').show();
                            $('#mensaje1').hide(); 
                        }
                    },
                    error: (data) => {
                        sut.error.show('mensajes', data.responseText);
                    },
                    statusCode: {
                        203: () => { window.location.reload(); }
                    }
                });
            },
            generar: (lista) => {
                var table = $('#tblLista');
                var tabla = table.children('tbody');
                tabla.children('tr').remove();
                var rows = '';
                var numero = (table.data('pagesize') * table.data('page')) - table.data('pagesize');
                $.each(lista, (i, r) => {
                    rows += sut.string.format('<tr><td class="colNro" >{0}</td>', ++numero);
                    rows += sut.string.format('<td>{0}</td>', r.Descripcion);
                    if (r.Estado == 1) {
                        var estadotitulo = 'Activo';
                    } else {
                        var estadotitulo = 'Baja';
                    }
                    rows += sut.string.format('<td style="text-align: center;width: 50px;">{0}</td>', estadotitulo);
                    rows += '<td style="text-align: center;width: 5px;">';
                    rows += '<div class="btn-group">';
                    if (r.RolId != 1 && r.RolId != 10){
                        rows += sut.string.format('<a  class="btn btn-success btn-xs"   data-placement="left" title="Editar Roles" href="javascript:sut.Roles.editar({0})" ><i class="fa fa-pencil"></i></a>', r.RolId);
                    }else
                    {
                        rows += sut.string.format('<a  class="btn btn-default btn-xs"   data-placement="left" title="Bloquedo" href="javascript:sut.Roles.bloqueo()" ><i class="fa fa-pencil"></i></a>', r.RolId);
                    }
                    rows += '</div>';
                    rows += '</td>';
                    rows += '<td style="text-align: center;width: 5px;">';
                    rows += '<div class="btn-group">';
                    if (r.RolId != 1 ){
                        rows += sut.string.format('<a class="btn btn-success btn-xs"   data-placement="left" title="Editar Menú" href="javascript:sut.Roles.vermenudinamico({0})" ><i class="fa fa-list-alt"></i></a>', r.RolId);
                    }
                    else
                    {
                        rows += sut.string.format('<a  class="btn btn-default btn-xs"   data-placement="left" title="Bloquedo" href="javascript:sut.Roles.bloqueo()" ><i class="fa fa-list-alt"></i></a>', r.RolId);
                    }
                    rows += '</div>';
                    rows += '</td>';
                    
                    rows += '<td style="text-align: center;width: 5px;">';
                    rows += '<div class="btn-group">';
                    if (r.RolId != 1 && r.RolId != 2 && r.RolId != 3 && r.RolId != 4 && r.RolId != 5 && r.RolId != 6 && r.RolId != 7 && r.RolId != 8 && r.RolId != 10){
                        rows += sut.string.format('<a class="btn btn-danger btn-xs"   data-placement="left" title="Eliminar Rol" href="javascript:sut.Roles.Eliminar({0})" ><i class="fa fa-trash"></i></a>', r.RolId);
                    }
                    else
                    {
                        rows += sut.string.format('<a  class="btn btn-default btn-xs"   data-placement="left" title="Bloquedo" href="javascript:sut.Roles.bloqueo()" ><i class="fa fa-trash"></i></a>', r.RolId);
                    }
                    rows += '</div>';
                    rows += '</td>';
                    rows += '</tr>';
                });
                sut.pagination.update(table);
                tabla.append(rows);
            },
            vermenudinamico: function (id) {
                window.location =  '@Url.Content("~/Seguridad/Roles/ListaMenuConfEditar/")'+id;
            },
            bloqueo: function () {
                showNotification('top', 'right', 'No se puede Editar este Perfil', 2);
            },
            Avanzar: (estado) => {
                if(estado==1)
                {
                    $('#modalAyudaAvatar1').modal('hide');
                    $('#modalSeguimiento').modal('show');
                    $('#modalSeguimiento').modal({backdrop: 'static', keyboard: false})
                }else
                {
                    $('#modalSeguimiento').modal('hide');
                    $('#modalAyudaAvatar1').modal({backdrop: 'static', keyboard: false})
                    $(".modal-backdrop").addClass("display-none");
                    $(".modal").css({
                        backgroundColor: "transparent"
                    });
                }
            },
            Eliminar: function (id) {

                sut.msg.confirm("Que desea eliminar", function () {
                $.ajax({
                    //url: "/Simplificacion/Recurso/Eliminar",
                    url: '@Url.Content("~/Seguridad/Roles/EliminarRol")',
                    type: "POST",
                    data: {id : id},
                    dataType: "json",
                    beforeSend: function () {
                        //sut.wait.appendOverlay('#boxLista');
                    },
                    success: function (data) {
                        //sut.wait.removeOverlay('#boxLista');
                        //sut.Recurso.listar(1);
                        $('#ventana-container').html(data);
                        location.reload();
                    },
                    error: function (data) {
                        sut.wait.removeOverlay('#boxLista');
                        sut.error.show('mensajes', data.responseText);
                    }
                });
                });
            },
            GuardarMenu: (menuid) => {

                $.ajax({
                    type: "GET",
                    //url: '/Seguridad/Miembro/Eliminar',
                    url: '@Url.Content("~/Seguridad/Miembro/GuardarMenu")',
                    data: { menuid: menuid,ruta:menuid },
                    cache: false,
                    beforeSend: () => {

                    },
                    success: (data) => {
                        if (data.exito) {

                            $('#modalAyudaAvatar1').modal('hide');
                            $('#modalSeguimiento').modal('show');
                            $('#modalSeguimiento').modal({backdrop: 'static', keyboard: false})
                            sut.Roles.listarayuda();

                        }
                    },
                    error: (result) => {
                        sut.error.show('mensajes', result.responseText);
                    },
                    statusCode: {
                        203: () => { window.location.reload(); }
                    }
                });

            },
            comenzar: function () {
                window.location = '@Url.Content("~/Seguridad/Usuario/ListaUsr")';
            }
            ,
            listarayuda: function () {

                $.ajax({
                    type: 'POST',
                    //url: '/Seguridad/Portada/EnviarAutorizacionMef',
                    url: '@Url.Content("~/Seguridad/Portada/GenerarListamenulista")',
                    data: {},
                    dataType: 'json',
                    beforeSend: function () {
                        $("#mensajes").hide();
                        sut.wait.appendProgress('.modal-footer');
                    },
                    complete: function () {
                        sut.wait.removeProgress('.modal-footer');
                    },
                    success: function (result) {
                        $('#modal-ventana').modal('hide');

                        var count = 1;
                        var countejecutar = 1;
                        var i=0;
                        var recorerr = 0;

                        debugger;
                        var rows = '';
                        var asdas= result.cantidad;
                        $('#colorNav').remove();

                        rows += '<nav id="colorNav" style="text-align:left;">'
                        rows += '<ul>'
                        for (i = recorerr; i < result.cantidad; i++)
                        {
                            if (result.menu[i].MenuId == 20)
                            {
                                if (@user.Sector == 80 || @user.Sector == 79)
                                {
                                    rows += '<li class="'+result.menu[i].Estilo+'" style="'+result.menu[i].Dimension+'">'
                                    rows += '<button class="pulse4-button" style="font-size:20px">'+countejecutar+'</button><br />'
                                    rows += '<span style="font-size: 15px;color: white;"> '+result.menu[i].Descripcion +'</span> <br />'
                                    rows += '<a href="'+ result.menu[i].url+'" class="'+result.menu[i].Gragico+'"></a>'

                                    rows += '<ul style="z-index:10">'

                                    for (var sub = i; sub < result.cantidad; sub++)
                                    {
                                        if (result.menu[sub].IdPadreMenu != result.menu[i].IdPadreMenu)
                                        {
                                            i = sub - 1;
                                            break;

                                        }
                                        rows += '<li style="'+result.menu[sub].Estilo+'"> <a href="'+result.menu[sub].url+'">'+result.menu[sub].Descripcion+'</a></li>'

                                    }
                                    rows += '</ul>'
                                    rows += '</li>'
                                }
                                else
                                {
                                    i = i + 1;
                                }
                            }
                            else if (result.menu[i].MenuId == 22)
                            {
                                if (@user.CodPadre == "1")
                                {
                                    rows += '<li class="'+result.menu[i].Estilo+'" style="'+result.menu[i].Dimension+'">'
                                    rows += '	<button class="pulse4-button" style="font-size:20px">'+countejecutar+'</button><br />'
                                    rows += '	<span style="font-size: 15px;color: white;"> '+result.menu[i].Descripcion+' </span> <br />'
                                    rows += '	<a href="'+result.menu[i].url+'" class="'+result.menu[i].Gragico+'"></a>'
                                    if (result.menu[i].IdPadreMenu != 0)
                                    {
                                        i = i + 1;
                                        rows += '<ul style="z-index:10">'

                                        for (var sub = i; sub < result.cantidad; sub++)
                                        {
                                            if (result.menu[sub].IdPadreMenu != result.menu[i].IdPadreMenu)
                                            {
                                                i = sub - 1;
                                                break;

                                            }
                                            rows += '<li style="'+result.menu[sub].Estilo+'" > <a href="'+result.menu[sub].url+'" > '+result.menu[sub].Descripcion+'</a></li>'

                                        }
                                        rows += '</ul>'
                                    }

                                    rows += '</li>'
                                }
                                else
                                {
                                    i = i + 1;
                                }
                            }
                            else
                            {
                                rows += '<li class="'+result.menu[i].Estilo+'" style="'+result.menu[i].Dimension+'">'
                                rows += ' <button class="pulse4-button" style="font-size:20px">'+countejecutar+'</button><br />'
                                rows += ' <span style="font-size: 15px;color: white;"> '+result.menu[i].Descripcion+' </span> <br />'
                                rows += ' <a href="'+result.menu[i].url+'" class="'+result.menu[i].Gragico+'"></a>'

                                if (result.menu[i].IdPadreMenu != 0)
                                {
                                    i = i + 1;
                                    rows += '<ul style="z-index:10">'

                                    for (var sub = i; sub < result.cantidad; sub++)
                                    {
                                        if (result.menu[sub].IdPadreMenu != result.menu[i].IdPadreMenu)
                                        {
                                            i = sub - 1;
                                            break;

                                        }
                                        rows += '<li style="'+result.menu[sub].Estilo+'"> <a href="'+result.menu[sub].url+'">'+result.menu[sub].Descripcion+'</a></li>'

                                    }
                                    rows += '</ul>'
                                }

                                rows += '</li>'
                            }
                            countejecutar = countejecutar + 1;

                        }
                        rows += '</ul>'
                        rows += '</nav>'

                        $("#colorNavContenido").append(rows);

                        document.querySelector('#nombremenu').innerText = result.nombremenu;


                    },
                    error: function (result) {
                        sut.error.show('mensajes', result.responseText);
                    }
                });

            },

        };

