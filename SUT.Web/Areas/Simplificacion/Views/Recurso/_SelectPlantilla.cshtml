        listar: function (table, page,tipo) {
            debugger;
            table = $('#' + table);
            var model = {
                'filtro.Nombre': $('#form-buscar-recurso input[name="Nombre'+tipo+'"]').val(),
                'tipo': tipo,
                'pageIndex': page,
                'pageSize': table.data('pagesize')
            };

            $.ajax({
                //url: '/Simplificacion/Recurso/GetAllLikePaginSinTipo',
                url: '@Url.Content("~/Simplificacion/Recurso/GetAllLikePaginSinTipo")',
                type: 'GET',
                data: model,
                dataType: 'json',
                beforeSend: function () {
                    sut.wait.appendProgress('#buscarRecurso'+tipo+' .modal-footer');
                },
                success: function (data) {
                    sut.wait.removeProgress('#buscarRecurso'+tipo+' .modal-footer');
                    table.data('page', page);
                    table.data('totalrows', data.totalRows);
                    sut.Recurso.generar(table, data.lista,tipo);
                    if (data.totalRows == 0)
                        $('#tblBuscarRecurso'+tipo+' tbody').append('<tr><td colspan="3" class="text-center text-bold">NO EXISTEN REGISTROS</td></td>');
                },
                error: function (data) {
                    sut.error.show('mensajes', data.responseText);
                }
            });
        },
        generar: function (table, lista,tipo) {
            debugger;
            var tabla = table.children('tbody');
            tabla.children('tr').remove();
            var rows = '';
            var numero = (table.data('pagesize') * table.data('page')) - table.data('pagesize');
            $.each(lista, function (i, r) {
                if (multi)
                {
                    rows += sut.string.format('<tr class="rowselect"><td><input id="chkbuscarRecurso' + tipo + '{0}" type="checkbox"  onclick="javascript:sut.Recurso.check(' + tipo + '{0})" value="{0}|{1}|{2}|{3}" /></td><td><input type="number"  name="orden' + tipo + '{0}"   maxlength="4" oninput="if(this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);"   id="orden' + tipo +'{0}" style="width: 45px;"></td><td>{1}</td></tr>',
                        r.RecursoId, r.Nombre, r.Tipo, r.TipoId);
                } else {
                    rows += sut.string.format('<tr class="rowselect"><td><input id="chkbuscarRecurso'+tipo+'{0}" type="hidden" value="{0}|{1}|{2}|{3}" /></td><td><input type="number" name="orden'+tipo+'{0}"   maxlength="4" oninput="if(this.value.length > this.maxLength) this.value = this.value.slice(0, this.maxLength);"  id="orden'+tipo+'{0}" style="width: 45px;"></td><td>{1}</td></tr>',
                        r.RecursoId, r.Nombre, r.Tipo, r.TipoId);
                }
            });
            sut.pagination.update(table);
            tabla.append(rows);

        },
