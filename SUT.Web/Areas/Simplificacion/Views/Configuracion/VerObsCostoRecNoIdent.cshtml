                rows += sut.string.format('<td colspan="12" ><textarea name="CostoPersonal2' + num + '" id="CostoPersonal2' + num + '" class="form-control" onkeypress="javascript:sut.RecursoCosto.ObsVervisualizarSustentoRecursoCosto(event,this);" style="border-color:red;background:#ff00002e;"></textarea></td>', '');
