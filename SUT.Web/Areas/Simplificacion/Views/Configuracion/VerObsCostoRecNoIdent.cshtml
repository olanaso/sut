@using Sut.Entities
@model List<RecursoCosto>
@{
    Expediente exp = ViewBag.Expediente as Expediente;
    Sut.Security.UsuarioInfo user = ViewBag.Usuario as Sut.Security.UsuarioInfo;
    List<Sut.Entities.Observacion> ObservacionCostoPersonal = ViewBag.ObservacionCostoPersonal as List<Sut.Entities.Observacion>;

    ViewBag.Icon = "fa fa-file-text-o";
    ViewBag.Title = "Sustento de Costos - Expediente " + exp.Codigo;
    ViewBag.Descripcion = "";
    var nombremultiple = ViewBag.nomoficial;
}

<div id="mensajes">
    @{Html.RenderPartial("_Error");}
</div>

<div class="box box-default">
    <div class="box-body">
        @{Html.RenderPartial("_VerObsMenuCostos", exp);} 
    </div>
</div>

<div id="boxLista" class="box box-danger">
    <div class="box-header with-border">
        <h3 class="box-title">@ViewBag.TipoRecurso</h3>
        <div class="box-tools pull-right">
            <a href="javascript:void(0)" class="btn btn-box-tool" data-toggle="tooltip" data-placement="bottom" title="Se lista los recursos asginados a las actividades de las Tablas ASME-VM.">
                <i class="fa fa-question-circle"></i>
            </a>
        </div>
    </div>
    <div class="box-body">
        @using (Html.BeginForm("", "", FormMethod.Post, new { @id = "form-recurso-costo" }))
        {
            <div class="table-responsive">
                <table id="tblLista" class="table table-bordered table-condensed table-striped" style="margin-bottom: 0px;font-size: 16px;">
                    <thead>
                        <tr>
                            <th style="width: 5%;"></th>
                            <th style="width: 20%;">Recurso</th>
                            <th style="width: 6%;">Cantidad</th>
                            <th style="width: 6%;">Unidad de Medida</th>
                            @if (ViewBag.Depreciacion)
                            {
                                <th style="width: 6%;">Costo Adquisición</th>
                                <th style="width: 6%;">% Depreciación</th>
                            }
                            <th style="width: 6%;">Costo Unitario</th>
                            <th style="width: 6%;">Costo Anual</th>
                            <th style="width: 14%;">Inductor</th>
                            <th style="width: 30%;">Doc. Sustento</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.Count() > 0)
                        {
                            for (int i = 0; i < Model.Count; i++)
                            { 
                                <tr>
                                    <td colspan="12">

                                        @*/*Inicio CostoPersonal*/*@
                                        <div class="form-group">
                                            <div class="col-sm-10 col-xs-12" style="text-align: left;">  
                                                @if (ObservacionCostoPersonal.Count != 0)
                                                {
                                                    var cont = 0;
                                                    var texto1 = "";
                                                    foreach (var item in ObservacionCostoPersonal)
                                                    {
                                                        var nombre = nombremultiple + i;

                                                        if (@item.Pantalla == nombremultiple && @item.NombreCampo == nombre)
                                                        {
                                                            cont = 1;

                                                            if (@item.TipoEstado == "1")
                                                            { 
                                                                texto1 = @item.Descripcion;
                                                            }
                                                            else if (@item.TipoEstado == "2")
                                                            { 
                                                                <script> 
                                                                    window.addEventListener("load", function() { 
                                                                        sut.RecursoCosto.textoobservacionCostoPersonal("@texto1.Replace("\n", " ")", "@item.Descripcion.Replace("\n", " ")", 4, "@exp.EstadoExpediente", @i); 
                                                                    });
                                                                </script>
                                                            } 
                                                            else if (@item.TipoEstado == "3")
                                                            {
                                                                <script> 
                                                                    window.addEventListener("load", function() { 
                                                                        sut.RecursoCosto.textosustentoCostoPersonal("@item.Descripcion.Replace("\n", " ")", 4, "@exp.EstadoExpediente", @i);
                                                                    });
                                                                </script>
                                                            }
                                                        }
                                                    }
                                                } 
                                            </div>
                                        </div>
                                        <div id="divCostoPersonal">
                                            <table id="divCostoPersonal0_@i" class="table table-condensed table-bordered table-striped" style="margin-bottom: 0px;font-size: 14px;">
                                                <thead> 
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                        @*/*Fin*/*@
                                    </td>
                                </tr> 
                                 

                                <tr>
                                    <td></td>
                                    <td>@Model[i].Recurso.Nombre</td>
                                    <td>@Html.TextBoxFor(model => model[i].Cantidad, new { @class = "form-control input-sm", @min = 0, @readonly = true })</td>
                                    <td>@Model[i].Recurso.UnidadMedida.Nombre</td>
                                    @if (ViewBag.Depreciacion)
                                    {
                                        <td>@Html.TextBoxFor(model => model[i].CostoAdquisicion, new { @class = "form-control input-sm", @min = 0, @readonly = true })</td>
                                        <td><span>@Model[i].Recurso.TipoDepreciacion.Valor02</span>%</td>
                                    }
                                    <td>@Html.TextBoxFor(model => model[i].CostoUnitario, new { @class = "form-control input-sm", @min = 0, @readonly = true })</td>
                                    <td><span>0</span></td>
                                    <td>
                                        @Html.HiddenFor(model => model[i].ExpedienteId)
                                        @Html.HiddenFor(model => model[i].RecursoCostoId)
                                        @Html.HiddenFor(model => model[i].RecursoId)
                                        @Html.HiddenFor(model => model[i].CostoAnual) 
                                        @{
                                            string Inductor = string.Empty;
                                            List<SelectListItem> lstInductor = ViewBag.ListaInductor as List<SelectListItem>;
                                            Inductor = lstInductor.First(x => x.Value == (Model[i].InductorId).ToString()).Text; 
                                        } 
                                        @Html.TextBox("NombreInductor"+ i, Inductor, new { @class = "form-control", @readonly = true })   
                                    </td>
                                    <td>@Html.TextBoxFor(model => model[i].DocSustento, new { @class = "form-control input-sm", @readonly = true }) </td>
                                    @Html.HiddenFor(model => model[i].Recurso.Nombre, new { id = "RecursoNombre" + i })
                                    @Html.HiddenFor(model => model[i].Cantidad, new { id = "Cantidad" + i }) 
                                    @Html.HiddenFor(model => model[i].CostoAdquisicion, new { id = "CostoAdquisicion" + i })
                                    @Html.HiddenFor(model => model[i].Recurso.TipoDepreciacion.Valor02, new { id = "Valor02" + i }) 
                                    @Html.HiddenFor(model => model[i].Recurso.UnidadMedida.Nombre, new { id = "RecursoUnidadMedidaNombre" + i })
                                    @Html.HiddenFor(model => model[i].CostoAnual, new { id = "CostoAnual" + i })
                                    @Html.HiddenFor(model => model[i].CostoUnitario, new { id = "CostoUnitario" + i }) 
                                    @Html.HiddenFor(model => model[i].DocSustento, new { id = "DocSustento" + i })
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        }
    </div>
    <div class="box-footer">
        <div class="pull-right">
            <a href="@Url.Content("~/Simplificacion/Configuracion/VerObsResumen/")?id=@exp.ExpedienteId&&entidadid=@ViewBag.entidadver" class="btn btn-danger"><i class="fa fa-arrow-left"></i> Volver</a>

        </div>
    </div>
</div>

@section Style {
    <style type="text/css">
        #tblLista thead tr th {
            vertical-align: middle !important;
            text-align: center !important;
            font-size: 0.8em;
        }

            #tblLista thead tr th:nth-child(2),
            #tblLista thead tr th:nth-child(4),
            #tblLista thead tr th:nth-child(5) {
                width: 100px;
            }

            #tblLista thead tr th:nth-child(6) {
                width: 300px;
            }

        #tblLista tbody tr td {
            vertical-align: middle !important;
            text-align: center !important;
            font-size: 0.9em;
        }

            #tblLista tbody tr td:nth-child(3) {
                width: 120px;
                text-align: center !important;
            }

            #tblLista tbody tr td:nth-child(5) {
                text-align: right !important;
            }

            #tblLista tbody tr td input[type="text"] {
                text-align: right !important;
            }
    </style>

    @if (ViewBag.Depreciacion)
    {
        <style type="text/css">
            #tblLista thead tr th:nth-child(6),
            #tblLista tbody tr td:nth-child(7) {
                width: 100px;
                text-align: right !important;
            }
        </style>
    }
}

@section Script {
    <script>
        sut.RecursoCosto = {
            ObsVervisualizarSustentoRecursoCosto: function (evt, input) {
                debugger;
                var key = window.Event ? evt.which : evt.keyCode;
                var chark = String.fromCharCode(key);
                var tempValue = input.value + chark;
                var id = input.id;
                var txt = $(evt.target);
                var valor = id.substring(14);
                $('#mensajeCostoPersonal_' + valor).hide();
                document.querySelector('#mensajeCostoPersonal_' + valor).innerText = '';
            },
            depreciacion: '@ViewBag.Depreciacion' == 'True',
            updateCalc: function () {
                var filas = $('#tblLista tbody tr');
                if (filas.size() > 0) {
                    $.each(filas, function (i, tr) {
                        var cantidad = Number($(tr).find('td input[name$="Cantidad"]').val());
                        if (!sut.RecursoCosto.depreciacion) {
                            var costoUnit = Number($(tr).find('td input[name$="CostoUnitario"]').val());
                            $(tr).find('td span').eq(0).html(sut.format.decimal(cantidad * costoUnit));
                            $(tr).find('td input[name$="CostoAnual"]').val(sut.format.decimal(cantidad * costoUnit));
                        } else {
                            var costoAdq = Number($(tr).find('td input[name$="CostoAdquisicion"]').val());
                            var tasa = Number($(tr).find('td span').eq(0).html())/100;
                            $(tr).find('td input[name$="CostoUnitario"]').val(sut.format.decimal(costoAdq * tasa)).attr('readonly', true);
                            $(tr).find('td span').eq(1).html(sut.format.decimal(costoAdq * tasa * cantidad));
                            $(tr).find('td input[name$="CostoAnual"]').val(sut.format.decimal(costoAdq * tasa * cantidad));
                        }
                    });
                }
            },

            guardar: function () {
                var valid = true;
                var inds = $('#tblLista tbody tr td select[name$="InductorId"] option:selected');
                $.each(inds, function (i, ind) {
                    if (Number($(ind).val()) == 0) valid = false;
                });

                if (!valid) sut.error.clientErrorShow('mensajes', 'Debe seleccionar un Inductor para cada Recurso.');

                if ($('#form-recurso-costo').valid() && valid) {
                    var model = $('#form-recurso-costo').serialize();
                    $.ajax({
                        type: 'POST',
                        //url: '/Simplificacion/Configuracion/GuardarRecursoCosto',
                        url:'@Url.Content("~/Simplificacion/Configuracion/GuardarRecursoCosto")',
                        data: model,
                        dataType: 'json',
                        beforeSend: function () {
                            $("#mensajes").hide();
                        },
                        complete: function () {

                        },
                        success: function (result) {
                            sut.msg.success(result.mensaje, function () {
                                window.location.reload();
                            });
                        },
                        error: function (result) {
                            sut.error.show('mensajes', result.responseText);
                        }
                    });
                }
            },

            init: function () {
                sut.RecursoCosto.updateCalc();
            },
            validarObservarExpCostoPersonal: function (idExpediente, numero,RecursoId,RecursoCostoId) {
                debugger;



                //var Descripcion = $('#RecursoNombre'+numero).val()+";:;"+$('#Cantidad'+numero).val()+";:;"+$('#RecursoUnidadMedidaNombre'+numero).val()+";:;"+$('#CostoUnitario'+numero).val()+";:;"+$('#CostoAnual'+numero).val()+";:;"+$('#DocSustento'+numero).val();
                var Descripcion = $('#RecursoNombre'+numero).val()+";:;"+$('#Cantidad'+numero).val()+";:;"+$('#RecursoUnidadMedidaNombre'+numero).val()+";:;"+$('#CostoUnitario'+numero).val()+";:;"+$('#CostoAnual'+numero).val()+";:;"+$('#NombreInductor'+numero).val()+";:;"+$('#DocSustento'+numero).val()+";:;"+$('#Valor02'+numero).val()+";:;"+$('#CostoAdquisicion'+numero).val();
                var NombreCampo = 'CostoPersonal';
                var model = {
                    'filtro.ExpedienteId': idExpediente,
                    'filtro.Descripcion': Descripcion,
                    'filtro.CodValidacion': '4',
                    'filtro.TipoEstado': '4',
                    'filtro.NombreCampo': '@nombremultiple' + numero,
                    'filtro.Pantalla': '@nombremultiple',
                    'filtro.Estado': '1',
                    'filtro.RecursoId': RecursoId,
                    'filtro.RecursoCostoId': RecursoCostoId,
                };
                $.ajax({
                    type: "POST",
                    //url: '/Simplificacion/Expediente/AgregarObservacion',
                    url:'@Url.Content("~/Simplificacion/Expediente/AgregarObservacion")',
                    data: model,
                    cache: false,
                    beforeSend: function () {
                        sut.wait.modal('ventana-container');
                        $('#modal-ventana').modal('show');
                    },
                    success: function (data) {
                        debugger;
                        $('#modal-ventana').modal('hide');
                        //$('#aValidarObservarExp' + NombreCampo + '_' + numero + '').css("display", "none");
                        $('#aCancelarGeneralExp' + NombreCampo + '_' +  numero + '').css("display", "");

                        $('#aObservarEditarExp' + NombreCampo + '_' + numero + '').css({ "display": "none" });
                        $('#aMasivoObservarExp' + NombreCampo + '_' + numero + '').css({ "display": "none" });
                        $('#mensajeCostoPersonal_' + numero).show();
                        document.querySelector('#mensajeCostoPersonal_' + numero).innerText = 'Se valido con éxito';

                    },
                    error: function (result) {
                        $('#modal-ventana').modal('hide');
                        sut.error.show('mensajes', result.responseText);
                    }
                });

                comentario = "";
            },
            CancelarGeneralExpCostoPersonal: function (idExpediente, procedimientoId, numero,RecursoId,RecursoCostoId) {
                debugger;

                var Descripcion = $('#RecursoNombre'+numero).val()+";:;"+$('#Cantidad'+numero).val()+";:;"+$('#RecursoUnidadMedidaNombre'+numero).val()+";:;"+$('#CostoUnitario'+numero).val()+";:;"+$('#CostoAnual'+numero).val()+";:;"+$('#NombreInductor'+numero).val()+";:;"+$('#DocSustento'+numero).val()+";:;"+$('#Valor02'+numero).val()+";:;"+$('#CostoAdquisicion'+numero).val();
                var NombreCampo = 'CostoPersonal';


                var model = {
                    'filtro.ExpedienteId': idExpediente,
                    'filtro.Descripcion': Descripcion,
                    'filtro.CodValidacion': '4',
                    'filtro.TipoEstado': '4',
                    'filtro.NombreCampo': '@nombremultiple' + numero,
                    'filtro.Pantalla': '@nombremultiple',
                    'filtro.Estado': '1',
                    'filtro.RecursoId': RecursoId,
                    'filtro.RecursoCostoId': RecursoCostoId,
                };


                $.ajax({
                    type: "POST",
                    //url: '/Simplificacion/Expediente/CancelarObservacion',
                    url:'@Url.Content("~/Simplificacion/Expediente/CancelarObservacion")',
                    data: model,
                    cache: false,
                    beforeSend: function () {
                        sut.wait.modal('ventana-container');
                        $('#modal-ventana').modal('show');
                    },
                    success: function (data) {
                        debugger;
                        $('#modal-ventana').modal('hide');


                        //$('#aValidarObservarExp' + NombreCampo +'_'+ numero + '').css("display", "");
                        $('#aObservarEditarExp' + NombreCampo + '_' + numero + '').css("display", "");
                        //$("#aSustentarObservarExpObjetivo").css("display","");
                        //$("#aMasivoObservarExpObjetivo").css("display","");

                        $('#aCancelarGeneralExp' + NombreCampo + '_' + numero + '').css({ "display": "none" });

                        $('#mensajeCostoPersonal_' + numero + '').hide();
                        document.querySelector('#mensajeCostoPersonal_' + numero + '').innerText = '';

                    },
                    error: function (result) {
                        $('#modal-ventana').modal('hide');
                        sut.error.show('mensajes', result.responseText);
                    }
                });

                comentario = "";
            },
            observareditarExpCostoPersonal: function (num) {
                debugger;
                var tabla = $('#divCostoPersonal0_' + num + ' tbody');
                debugger;
                var NombreCampo = 'CostoPersonal';
                var rows = '';

                var monto0= sut.format.decimal($('#CostoAnual'+num).val() / 12);
                var monto1= sut.format.decimal(($('#CostoAnual'+num).val() / 12) / 14400);


                rows += sut.string.format('<tr data-index="{0}" id="trdivCostoPersonal0' + num + '">', num);
                rows +='<td style="width: 5%;"> Descripción: </td>';
                if ('@ViewBag.Depreciacion' == 'True') {
                    rows += '<td style="border-color:#00a65a00;background:#00a65a36;border: 1px solid #f4f4f4;width: 20%;">';
                    rows += $('#RecursoNombre' + num).val();
                    rows += '</td>';
                } else {
                    rows += '<td style="border-color:#00a65a00;background:#00a65a36;border: 1px solid #f4f4f4;width: 20%;">';
                    rows += $('#RecursoNombre' + num).val();
                    rows += '</td>';
                }

                rows +='<td style="border-color:#00a65a00;background:#00a65a36;border: 1px solid #f4f4f4;width: 6%;">';
                rows +=sut.format.decimal($('#Cantidad'+num).val());
                rows += '</td>';
                rows +='<td style="border-color:#00a65a00;background:#00a65a36;border: 1px solid #f4f4f4;width: 6%;">';
                rows += $('#RecursoUnidadMedidaNombre'+num).val() ;
                rows += '</td>';
                debugger;
                if ('@ViewBag.Depreciacion' == 'True') {
                    rows += '<td style="border-color:#00a65a00;background:#00a65a36;border: 1px solid #f4f4f4;width: 6%;">';
                     rows += $('#CostoAdquisicion' + num).val();
                    rows += '</td>';
                    rows += '<td style="border-color:#00a65a00;background:#00a65a36;border: 1px solid #f4f4f4;width: 6%;">';
