<div class="row">
    <div class="col-xs-12">
        <div id="boxLista" class="box box-danger">
            <div class="box-header with-border">
    <div class="col-sm-11 col-xs-12">
         <h3 class="box-title"> Procedimientos Administrativos y/o Servicios prestados en exclusividad</h3> 
    </div>
    @if (@user.Rol == 6 || @user.Rol != 7 || @user.Rol != 8)
    {

        <div style="position: absolute;right: -8px;top: -13px;  padding-top: 15px;  margin-bottom: 0;">
            <div class="btn-group">
                <div class="col-sm-12 col-xs-12">
                    @if (ViewBag.desactivar != 0)
                    {
                        if (user.Rol == 6)
                        {
                            if (((Model.EstadoRactificador != EstadoExpediente.Aprobado && Model.EstadoExpediente != EstadoExpediente.Observado)))
                            {
                                <a class="btn btn-success" id="Estado" data-toggle="modal" data-target="#AddDate"><i class="fa fa-check-circle"></i> Concluir Sección</a>
                            }
                        }
                        else if (user.Rol == 8)
                        {
                            if (((Model.EstadoExpediente == EstadoExpediente.Publicado)) )
                            {
                                <a class="btn btn-success" id="Estado" data-toggle="modal" data-target="#AddDate"><i class="fa fa-check-circle"></i> Concluir Sección</a>
                            }
                        }
                        else if (user.Rol == 7)
                        {
                            if (((Model.EstadoExpediente != EstadoExpediente.Aprobado && Model.EstadoExpediente != EstadoExpediente.Publicado && Model.EstadoExpediente != EstadoExpediente.Observado)))
                            {
                                if (user.EstadoMinistrio == 1 && Model.EstadoEvaluadorMinisterio != EstadoExpediente.Aprobado)
                                {
                                    <a class="btn btn-success" id="Estado" data-toggle="modal" data-target="#AddDate"><i class="fa fa-check-circle"></i> Concluir Sección</a>
                                }
                                else if (user.EntidadId == 4105 && Model.EstadoEvaluadorPCM != EstadoExpediente.Aprobado)
                                {
                                    <a class="btn btn-success" id="Estado" data-toggle="modal" data-target="#AddDate"><i class="fa fa-check-circle"></i> Concluir Sección</a>
                                }
                                else if (user.EntidadId == 18497 && Model.EstadoEvaluadorMEF != EstadoExpediente.Aprobado)
                                {
                                    <a class="btn btn-success" id="Estado" data-toggle="modal" data-target="#AddDate"><i class="fa fa-check-circle"></i> Concluir Sección</a>
                                }
                            }
                            else if (user.EntidadId == 4105 && Model.EstadoExpediente == EstadoExpediente.Aprobado && Model.EstadoEvaluadorMinisterio == EstadoExpediente.Aprobado && Model.EstadoExpediente != EstadoExpediente.Publicado)
                            {
                                if (user.EntidadId == 4105 && Model.EstadoEvaluadorPCM != EstadoExpediente.Aprobado)
                                {
                                    <a class="btn btn-success" id="Estado" data-toggle="modal" data-target="#AddDate"><i class="fa fa-check-circle"></i> Concluir Sección</a>
                                }
                            }
                            else if (user.EntidadId == 18497 && Model.EstadoExpediente == EstadoExpediente.Aprobado && Model.EstadoEvaluadorMinisterio == EstadoExpediente.Aprobado && Model.EstadoExpediente != EstadoExpediente.Publicado)
                            {
                                if (user.EntidadId == 18497 && Model.EstadoEvaluadorMEF != EstadoExpediente.Aprobado)
                                {
                                    <a class="btn btn-success" id="Estado" data-toggle="modal" data-target="#AddDate"><i class="fa fa-check-circle"></i> Concluir Sección</a>
                                }
                            }
                        }
                    }
                    <a class="btn btn-facebook" id="filtro" href="javascript:sut.Procedimiento.filtro()"><i class="fa fa-filter"></i> Filtros</a>
                    <a class="btn btn-facebook" id="quitarfiltro" href="javascript:sut.Procedimiento.quitarfiltro()"><i class="fa fa-filter"></i> Quitar Filtro</a>

                </div>
            </div>
        </div>
        if (ViewBag.desactivar != 0)
        {

            <div style="position: absolute;right: 500px;top: -13px;  padding-top: 15px;  margin-bottom: 0;">
                <div class="btn-group">
                    <div class="col-sm-12 col-xs-12">

                        @if (ViewBag.RevInfCdno == 1)
                        {
                            <label class="btn btn-primary btn-xs" style="width:120px" id="Inf"><i class="fa fa-spinner"></i> Inf.Cdno. <br> En Proceso</label>
                        }
                        else if (ViewBag.RevInfCdno == 2)
                        {
                            <label class="btn btn-success btn-xs" style="width:120px" id="Inf"><i class="fa fa-check-square-o"></i> Inf.Cdno. <br> Terminado</label>
                        }
                        else if (ViewBag.RevInfCdno == 3)
                        {
                            <label class="btn btn-danger btn-xs" style="width:120px" id="Inf"><i class="fa fa-stop"></i> Inf.Cdno. <br> Detenido</label>
                        }

                        @if (ViewBag.RevDatosGenerales == 1)
                        {
                            <label class="btn btn-primary btn-xs" style="width:120px" id="Inf"><i class="fa fa-spinner"></i>  Datos Generales <br> En Proceso</label>
                        }
                        else if (ViewBag.RevDatosGenerales == 2)
                        {
                            <label class="btn btn-success btn-xs" style="width:120px" id="Inf"><i class="fa fa-check-square-o"></i> Datos Generales <br> Terminado</label>
                        }
                        else if (ViewBag.RevDatosGenerales == 3)
                        {
                            <label class="btn btn-danger btn-xs" style="width:120px" id="Inf"><i class="fa fa-stop"></i> Datos Generales <br> Detenido</label>
                        }

                        @if (ViewBag.RevSutentoTecnico == 1)
                        {
                            <label class="btn btn-primary btn-xs" style="width:120px" id="Inf"><i class="fa fa-spinner"></i> STL <br> En Proceso</label>
                        }
                        else if (ViewBag.RevSutentoTecnico == 2)
                        {
                            <label class="btn btn-success btn-xs" style="width:120px" id="Inf"><i class="fa fa-check-square-o"></i> STL <br> Terminado</label>
                        }
                        else if (ViewBag.RevSutentoTecnico == 3)
                        {
                            <label class="btn btn-danger btn-xs" style="width:120px" id="Inf"><i class="fa fa-stop"></i> STL <br> Detenido</label>
                        }

                        @if (ViewBag.RevTablaAsme == 1)
                        {
                            <label class="btn btn-primary btn-xs" style="width:120px" id="Inf"><i class="fa fa-spinner"></i> Tabla Asme <br> En Proceso</label>
                        }
                        else if (ViewBag.RevTablaAsme == 2)
                        {
                            <label class="btn btn-success btn-xs" style="width:120px" id="Inf"><i class="fa fa-check-square-o"></i> Tabla Asme <br> Terminado</label>
                        }
                        else if (ViewBag.RevTablaAsme == 3)
                        {
                            <label class="btn btn-danger btn-xs" style="width:120px" id="Inf"><i class="fa fa-stop"></i> Tabla Asme <br> Detenido</label>
                        }

                        @if (ViewBag.RevSustentoCosto == 1)
                        {
                            <label class="btn btn-primary btn-xs" style="width:120px" id="Inf"><i class="fa fa-spinner"></i> Sustento Costo <br> En Proceso</label>
                        }
                        else if (ViewBag.RevSustentoCosto == 2)
                        {
                            <label class="btn btn-success btn-xs" style="width:120px" id="Inf"><i class="fa fa-check-square-o"></i> Sustento Costo <br> Terminado</label>
                        }
                        else if (ViewBag.RevSustentoCosto == 3)
                        {
                            <label class="btn btn-danger btn-xs" style="width:120px" id="Inf"><i class="fa fa-stop"></i> Sustento Costo <br> Detenido</label>
                        }

                    </div>
                </div>
            </div>

        }
                <div class="col-sm-12 col-xs-12" id="idFiltros">
                    <br />
                    <label class="col-sm-3 col-xs-3 control-label">Calificación :</label>
                    <div class="col-sm-3 col-xs-3">
                        @Html.DropDownListFor(model => @fil.Clasificacion, ViewBag.Calificacion as List<SelectListItem>, new { @class = "form-control " })
                    </div>

