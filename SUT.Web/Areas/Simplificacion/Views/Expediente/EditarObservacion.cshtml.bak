                        $("#quitarfiltro").hide();
                        $("#filtro").show();
                    });
                </script>
                }
                else
                {
                <script>

                    window.addEventListener("load", function () {
                        $("#idFiltros").show();
                        $("#quitarfiltro").show();
                        $("#filtro").hide();
                        });
                </script>
                }
            }
            else
            {
                <script>

                    window.addEventListener("load", function () {
                        $("#idFiltros").hide();
                        $("#quitarfiltro").hide();
                        $("#filtro").show();
                    });
                </script>
                }
            }
